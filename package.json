{
  "name": "phit-api",
  "private": true,
  "license": "UNLICENSED",
  "version": "0.2.1",
  "scripts": {
    "build": "nest build",
    "build:email": "email build --dir=./src/modules/sendgrid/templates",
    "cli": "ts-node -r tsconfig-paths/register src/cli/index.ts",
    "eslint": "eslint \"{src,tests}/**/*.{ts,tsx}\"",
    "format": "prettier --write \"{src,tests}/**/*.{ts,tsx}\" && eslint --fix \"{src,tests}/**/*.{ts,tsx}\"",
    "prettier:check": "prettier --check \"{src,tests}/**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"{src,tests}/**/*.{ts,tsx}\"",
    "recreate-dev-env": "docker compose --project-name=fieldhouse rm -fs && docker compose --project-name=fieldhouse up -d",
    "repl": "npm run start -- --entryFile repl",
    "dev": "nest start --watch",
    "start": "nest start --watch",
    "start:email": "email dev --dir=./src/modules/sendgrid/templates",
    "test": "jest --config jest.config.ts",
    "typecheck": "tsc --project tsconfig.json"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@azure/msal-node": "^5.0.2",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.3.7",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.7",
    "@nestjs/platform-express": "^10.3.7",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/terminus": "^10.2.3",
    "@prisma/client": "^7.3.0",
    "@prisma/internals": "^7.3.0",
    "axios": "^1.6.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^9.0.3",
    "nanoid": "^3.3.7",
    "openid-client": "^5.6.5",
    "pg": "^8.11.5",
    "pino": "^8.20.0",
    "pino-pretty": "^11.0.0",
    "prisma": "^7.3.0",
    "qs": "^6.14.1",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.16",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.18.8",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.2.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-unicorn": "^52.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nest-commander": "^3.10.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.3",
    "typescript-eslint": "7.0"
  },
  "overrides": {
    "tsconfig-paths": "^4.2.0"
  }
}
